{
  "compilerOptions": {
    "target": "es2016",
    "jsx": "react",
    "module": "es2015",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "experimentalDecorators": true,
    "preserveConstEnums": true,
    "allowJs": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "strictNullChecks": false,
    "baseUrl": "./",
    "skipLibCheck": true,
    "sourceMap": true,
    "paths": {
      "src/*": ["./src/*"],
      "components/*": ["./src/components/*"]
    }
  }
}
